  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/chai": "^4.1.3",
    "@types/chai-as-promised": "^7.1.0",
    "@types/joi": "^13.0",
    "@types/lodash": "^4.14",
    "@types/node": "^8.10",
    "@types/sinon-chai": "^2.7.28",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "chai-interface": "^2.0.3",
    "chance": "^1.0.16",
