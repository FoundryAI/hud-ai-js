  "author": "FoundryAI Engineering",
  "license": "MIT",
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/chai": "^4.0.3",
    "@types/chai-as-promised": "^7.1.0",
    "@types/joi": "^10.4.1",
    "@types/lodash": "^4.14.70",
    "@types/node": "^8.0.25",
    "@types/sinon-chai": "^2.7.28",
    "chai": "^4.1.1",
    "chai-as-promised": "^7.1.1",
    "chai-interface": "^2.0.3",
    "chance": "^1.0.10",
    "mocha": "^3.5.0",
    "mocha-typescript": "^1.1.7",
    "moment": "^2.18.1",
    "nock": "^9.0.14",
    "nyc": "^11.1.0",
    "sinon": "^3.2.0",
    "sinon-chai": "^2.13.0",
    "typescript": "^2.5.2",
    "typings": "^2.1.1"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "bluebird": "^3.5.0",
    "date-fns": "^1.28.5",
    "joi": "^10.6.0",
    "lodash": "^4.17.4",
    "ts-node": "^3.3.0"
  },
  "nyc": {
    "include": [
      "lib/**/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "sourceMap": true,
    "instrument": true
